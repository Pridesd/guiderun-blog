{
  "format": "page",
  "id": "blog_post",
  "label": "Blog Post",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "id"
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Title",
          "placeholder": "제목을 입력해주세요"
        }
      },
      "title_image": {
        "type": "Image",
        "config": {
          "label": "Title Image",
          "constraint": {},
          "thumbnails": []
        }
      },
      "category": {
        "type": "Select",
        "config": {
          "label": "Category",
          "placeholder": "해당 게시글이 속하는 카테고리를 선택해주세요",
          "options": ["훈련", "서비스"],
          "default_value": "훈련"
        }
      },
      "author": {
        "type": "Text",
        "config": {
          "label": "Author",
          "placeholder": "작성자를 입력해주세요. 미입력 시 \"팀 가이드런\" 이 입력됩니다. "
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "rich_text": {
              "type": "SharedSlice"
            }
          }
        }
      }
    },
    "SEO & Metadata": {
      "meta_title": {
        "config": {
          "label": "Meta Title",
          "placeholder": "A title of the page used for social media and search engines"
        },
        "type": "Text"
      },
      "meta_description": {
        "config": {
          "label": "Meta Description",
          "placeholder": "A brief summary of the page"
        },
        "type": "Text"
      },
      "meta_image": {
        "config": {
          "constraint": {
            "height": 1260,
            "width": 2400
          },
          "label": "Meta Image",
          "thumbnails": []
        },
        "type": "Image"
      }
    }
  }
}
